   "source": [
    "word = input('Enter a word of at most 5 alphabets: ')\n",
    "word = word.strip().lower()\n",
    "length = len(word)\n",
    "flag = 1\n",
    "if length > 5:\n",
    "    flag = 0\n",
    "elif word != word[::-1]:\n",
    "    flag = 0\n",
    "\n",
    "print(['Is not a palindrome','Is a palindrome'][flag])"
   ],
